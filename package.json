{
  "name": "uyoutodo",
  "version": "3.0.3",
  "private": false,
  "author": {
    "name": "Anthony Lu",
    "email": "taruruchan0@gmail.com"
  },
  "license": "MIT",
  "main": "electron/main.js",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "electron:servewin": "concurrently -k \"pnpm vue-devtools\" \"pnpm dev\" \"pnpm electron\"",
    "electron:servemac": "concurrently -k \"pnpm dev\" \"pnpm electron:mac\"",
    "electron:servelinux": "concurrently -k \"pnpm vue-devtools\" \"pnpm dev\" \"pnpm electron:linux\"",
    "electron:buildwin": "vite build && electron-builder",
    "electron:buildwinarm": "vite build && electron-builder --win --arm64",
    "electron:buildmac": "vite build && electron-builder --config.npmRebuild=false -c.extraMetadata.main=electron/mac/mainMac.js",
    "electron": "wait-on tcp:3000 && cross-env NODE_ENV=development electron .",
    "electron:mac": "wait-on tcp:3000 && cross-env NODE_ENV=development electron electron/mac/mainMac.js",
    "electron:linux": "wait-on tcp:3000 && cross-env NODE_ENV=development electron electron/mac/mainMac.js --no-sandbox",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "@electron/remote": "^2.1.2",
    "electron-store": "^8.1.0",
    "mica-electron": "^1.5.15"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.0.0",
    "@electron/rebuild": "^3.6.0",
    "@iconify-json/circle-flags": "^1.1.47",
    "@iconify-json/emojione-v1": "^1.1.10",
    "@iconify-json/f7": "^1.1.0",
    "@iconify-json/flagpack": "^1.1.14",
    "@iconify-json/fluent": "^1.1.55",
    "@iconify-json/icon-park": "^1.1.13",
    "@iconify-json/logos": "^1.1.42",
    "@iconify-json/mdi": "^1.1.66",
    "@iconify-json/ph": "^1.1.13",
    "@stylistic/eslint-plugin": "^2.7.2",
    "@types/node": "^20.14.0",
    "@unocss/eslint-plugin": "^0.62.3",
    "@unocss/preset-icons": "^0.62.3",
    "@unocss/transformer-attributify-jsx": "^0.62.3",
    "@vitejs/plugin-vue": "^5.1.3",
    "@vitejs/plugin-vue-jsx": "^4.0.1",
    "@vue-vine/eslint-parser": "^0.1.20",
    "@vue/devtools": "^7.3.8",
    "@vueuse/components": "^11.0.3",
    "@vueuse/core": "^11.0.3",
    "concurrently": "^7.2.2",
    "cross-env": "^7.0.3",
    "electron": "^32.0.1",
    "electron-builder": "^25.0.5",
    "element-plus": "^2.8.1",
    "eslint": "^9.9.1",
    "eslint-plugin-format": "^0.1.2",
    "floating-vue": "5.2.2",
    "lodash": "^4.17.21",
    "mitt": "^3.0.1",
    "moment": "^2.30.1",
    "sass": "^1.53.0",
    "typescript": "^5.5.4",
    "unocss": "^0.62.3",
    "unplugin-element-plus": "^0.8.0",
    "unplugin-vue-macros": "^2.11.6",
    "vite": "^5.4.3",
    "vite-plugin-electron-renderer": "^0.14.5",
    "vue": "^3.5.0",
    "vue-i18n": "9.14.0",
    "vue-router": "^4.4.3",
    "vue-tsc": "^2.1.4",
    "vue-vine": "^0.1.20",
    "vue-vine-tsc": "^0.0.2",
    "vue3-perfect-scrollbar": "^2.0.0",
    "wait-on": "^7.2.0"
  },
  "volta": {
